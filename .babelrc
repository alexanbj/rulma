{
  "presets": [
    "react",
    ["env", { "es2015": { "modules": false } }]
  ],
  "plugins": [
    ["transform-object-rest-spread", { "useBuiltIns": true }]
  ],
  "env": {
    "commonjs": {
      "plugins": [
        "transform-es2015-modules-commonjs"
      ]
    },
    "test": {
      "plugins": [
        ["transform-es2015-modules-commonjs"]
      ]
    }
  },
}
